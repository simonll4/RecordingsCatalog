# Environment Configuration

# Database
DATABASE_URL="postgresql://edge_user:edge_password@localhost:5432/edge_agent"

# PostgreSQL Docker Configuration
POSTGRES_DB=edge_agent
POSTGRES_USER=edge_user
POSTGRES_PASSWORD=edge_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Capture settings
CAPTURE_PROVIDER=ffmpeg  # opencv | ffmpeg

# Detector settings  
DETECTOR=onnx  # onnx | stub

# Application
NODE_ENV=development

# Logging
LOG_LEVEL=info  # debug | info | warn | error

# Camera Configuration
DEFAULT_CAMERA_DEVICE=0
DEFAULT_STREAM_WIDTH=640
DEFAULT_STREAM_HEIGHT=480
DEFAULT_STREAM_FPS=30